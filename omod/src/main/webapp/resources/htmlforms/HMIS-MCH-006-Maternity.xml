<htmlform formName="HMIS MCH 006: Integrated Maternity Register" formDescription="Admission into a clinic for a pregnant woman to give birth" formEncounterType="a9f11592-22e7-45fc-904d-dfe24cb1fc67" formUuid="0b946c41-957c-4aeb-9b1a-da3983c0d163" formVersion="3.0.0" formAddMetadata="yes" formUILocation="patientDashboard.visitActions" formOrder="6" formIcon="icon-medkit" formShowIf="(visit.active || !visit.active) &amp;&amp; patient.person.dead==false &amp;&amp; patient.person.gender=='F' &amp;&amp;sessionLocation.uuid=='e9bc61b5-69ff-414b-9cf0-0c22d6dfca2b'" formDisplayStyle="Standard" formLabel="HMIS-MCH-006-Maternity Form">


	<script type="text/javascript">
		if (jQuery) {

		    jq(document).ready(function () {

				//Block Multiple encounter entry on same date
				blockEncounterOnSameDateEncounter(jq("#encounterDate").find("input[type=hidden]"),"block");

				// Validate MUAC
                var age  = <lookup complexExpression="$patient.age" />; 
                getMUACCodeFromMUACScoreByAge(age, "muac-score", "muac-code");

                disable_fields("other-complications-of-pregnancy");
                disable_fields("other-management-procedure");
                disable_fields("other-source-of-warmth");
                disable_fields("other-ward-or-department");
                disable_fields("transferred-by");
				disable_fields("transferred-to");
                disable_fields("other-mode-of-delivery");

                //enable/disable other management procedure
                jq("#165443").change(function () {
	                if(jq(this).find("select").find(":selected").val() == "5622") {
	                    enable_fields("other-management-procedure");
                    } else {
                    	disable_fields("other-management-procedure");
                    }
                });

                //enable/disable other source of warmth
                jq("#165446").change(function () {
	                if(jq(this).find("select").find(":selected").val() == "90002") {
	                    enable_fields("other-source-of-warmth");
                    } else {
                    	disable_fields("other-source-of-warmth");
                    }
                });

                //enable/disable other ward/department
                jq("#99334").change(function () {
	                if(jq(this).find("select").find(":selected").val() == "165449") {
	                    enable_fields("other-ward-or-department");
                    } else {
                    	disable_fields("other-ward-or-department")
                    }
                });

                //enable/disable transferred by
                jq("#99334").change(function () {
	                if(jq(this).find("select").find(":selected").val() == "165449") {
	                    enable_fields("transferred-by");
                    } else {
                    	disable_fields("transferred-by")
                    }
                });

		       //enable/disable transferred to
		         jq("#99334").change(function () {
					if(jq(this).find("select").find(":selected").val() == "165449") {
					 	enable_fields("transferred-to");
					} else {
					   disable_fields("transferred-to")
					}
		          });

                //enable/disable other mode of delivery
                jq("#5630").change(function () {
	                if(jq(this).find("select").find(":selected").val() == "5622") {
	                    enable_fields("other-mode-of-delivery");
                    } else {
                    	disable_fields("other-mode-of-delivery")
                    }
                });

                //enable/disable other complications of pregnancy
                jq('.pregnancy-complications').change(function () {

                	if (jq(this).find('#5622').find(':checkbox').prop('checked')) {

                        enable_fields("other-complications-of-pregnancy");
                        disable_fields("50");
                        disable_fields("228");
                        disable_fields("230");
                        disable_fields("135361");
                        disable_fields("47");
                        disable_fields("115036");
                        disable_fields("130");
                        disable_fields("226");
                        disable_fields("148834");
                        disable_fields("127259");
                        disable_fields("46");

                    } else {

                        disable_fields("other-complications-of-pregnancy");
                        enable_fields("50");
                        enable_fields("228");
                        enable_fields("230");
                        enable_fields("135361");
                        enable_fields("47");
                        enable_fields("115036");
                        enable_fields("130");
                        enable_fields("226");
                        enable_fields("148834");
                        enable_fields("127259");
                        enable_fields("46");
                    }
				});

				/*determine viral load status*/
                jq("#viral-load-results-copies").change(function () {
                    jq("#viral-load-status").show();
                    var vl_copies = jq(this).find("input").val();
                    if (vl_copies ==" " || vl_copies == 0) {
                        jq("#viral-load-status").hide();
                    }
                    if (vl_copies &lt; 1000) {
                        jq("#viral-load-status").html("Suppressed");
                    } else if (vl_copies &gt;= 1000) {
                        jq("#viral-load-status").html("Non-suppressed");
                    } else {
                        jq("#viral-load-status").hide();
                    }
                });

				//addRemovePrescription();

                $allRows = jq(this).find('.visit-follow-up');
                $allRows.hide();
                jq('#1-removeEntry').remove();
                jq('#10-addEntry').remove();
                jq('#1-toggleContainer').show();

                jq('#11-removeEntry').remove();
                jq('#20-addEntry').remove();
                jq('#11-toggleContainer').show();

                jq('.addEntry').on("click", function() {
                    var correctedAddButtonId = parseFloat(this.id) + 1;
                    var contentAddId = "#" + correctedAddButtonId + "-toggleContainer";
                    jq(contentAddId).toggle(true);
                    jq('#' + this.id).toggle(false); jq('#' + parseFloat(this.id) + '-removeEntry').toggle(true);
                    return false;
                });

                jq('.removeEntry').on("click", function() {
                    var correctedRemoveButtonId = parseFloat(this.id) - 1;
                    var contentAddId = "#" + parseFloat(this.id) + "-toggleContainer";
                    jq(contentAddId).toggle(false);
                    jq( ':input:not(:button)', contentAddId).val([]);
                    jq('#' + correctedRemoveButtonId + '-addEntry').toggle(true); jq('#' + correctedRemoveButtonId + '-removeEntry').toggle(true);

                    return false;
                });
			});
		}
	</script>

